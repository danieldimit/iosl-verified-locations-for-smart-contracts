{
   "swagger": "2.0",
   "info": {
      "description": "Blockchain IOSL - Proof of Location",
      "version": "1.0.0",
      "title": "BLOCKHAIN IOSL",
      "termsOfService": "",
      "contact": {
         "email": "singh.1@campus.tu-berlin.de"
      }
   },
   "host": "localhost:4000",
   "basePath": "/api/v1",
   "tags": [
      {
         "name": "account",
         "description": "Everything about your account"
      },
      {
         "name": "owner",
         "description": "Access to owner APIs"
      },
      {
         "name": "renter",
         "description": "Access to renter APIs"
      }
   ],
   "schemes": [
      "http"
   ],
   "paths": {
      "/account/": {
         "get": {
            "tags": [
               "account"
            ],
            "summary": "Get All the accounts in blockchain",
            "description": "",
            "operationId": "getAccounts",
            "produces": [
               "application/json"
            ],
            "responses": {
               "200": {
                  "description": "successful operation"
               }
            }
         }
      },
      "/account/{address}": {
         "get": {
            "tags": [
               "account"
            ],
            "summary": "Get associated address to the accounts in blockchain",
            "description": "",
            "operationId": "getAccountDetails",
            "produces": [
               "application/json"
            ],
            "parameters": [
               {
                  "name": "address",
                  "in": "path",
                  "description": "address",
                  "required": true,
                  "type": "string",
                  "format": "string"
               }
            ],
            "responses": {
               "200": {
                  "description": "successful operation"
               }
            }
         }
      },
      "/owner/{address}": {
         "get": {
            "tags": [
               "owner"
            ],
            "summary": "Deploy Owener Contract",
            "description": "",
            "operationId": "deployContract",
            "produces": [
               "application/json"
            ],
            "parameters": [
               {
                  "name": "address",
                  "in": "path",
                  "description": "address",
                  "required": true,
                  "type": "string",
                  "format": "string"
               }
            ],
            "responses": {
               "200": {
                  "description": "successful operation"
               }
            }
         },
         "post": {
            "tags": [
               "owner"
            ],
            "summary": "Add a New Car",
            "description": "",
            "operationId": "addNewCar",
            "produces": [
               "application/json"
            ],
            "parameters": [
               {
                  "name": "address",
                  "in": "path",
                  "description": "address",
                  "required": true,
                  "type": "string",
                  "format": "string"
               },
               {
                  "in": "body",
                  "name": "body",
                  "description": "Add a New Car",
                  "required": true,
                  "schema": {
                     "$ref": "#/components/schemas/addmewcar"
                  }
               }
            ],
            "responses": {
               "200": {
                  "description": "successful operation"
               }
            }
         },
         "delete": {
            "tags": [
               "owner"
            ],
            "summary": "Delete a Car from the contract",
            "description": "",
            "operationId": "deleteCar",
            "produces": [
               "application/json"
            ],
            "parameters": [
               {
                  "name": "address",
                  "in": "path",
                  "description": "address",
                  "required": true,
                  "type": "string",
                  "format": "string"
               },
               {
                  "in": "body",
                  "name": "body",
                  "description": "Delete a  Car",
                  "required": true,
                  "schema": {
                     "$ref": "#/components/schemas/deletecar"
                  }
               }
            ],
            "responses": {
               "200": {
                  "description": "successful operation"
               }
            }
         }
      },
      "/owner/getcars/{address}": {
         "get": {
            "tags": [
               "owner"
            ],
            "summary": "get all the cars of the owner",
            "description": "",
            "operationId": "getAllCars",
            "produces": [
               "application/json"
            ],
            "parameters": [
               {
                  "name": "address",
                  "in": "path",
                  "description": "address",
                  "required": true,
                  "type": "string",
                  "format": "string"
               }
            ],
            "responses": {
               "200": {
                  "description": "successful operation"
               }
            }
         }
      },
      "/renter/": {}
   },
   "components": {
      "schemas": {
         "addmewcar": {
            "title": "Adding a new car",
            "type": "object",
            "properties": {
               "cargsm": {
                  "type": "string",
                  "description": "cargsm id"
               }
            },
            "required": [
               "cargsm"
            ]
         },
         "deletecar": {
            "title": "delete a car",
            "type": "object",
            "properties": {
               "caraddress": {
                  "type": "string",
                  "description": "Address of car"
               }
            },
            "required": [
               "caraddress"
            ]
         }
      }
   }
}